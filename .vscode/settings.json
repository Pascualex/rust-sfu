{
    "files.exclude": {
        "**/Cargo.lock": true,
        "**/target": true,
    },
    "editor.codeLens": false,
    "rust-analyzer.checkOnSave.overrideCommand": [
        "cargo",
        "clippy",
        "--message-format=json",
        "--",
        "-A",
        "clippy::module-inception"
    ],
    "cSpell.words": [
        "addrs",
        "alpn",
        "arraybuffer",
        "bincode",
        "bindgen",
        "clippy",
        "datagram",
        "dtls",
        "fmtp",
        "Hasher",
        "Kbps",
        "keepalive",
        "ontrack",
        "pemfile",
        "playsinline",
        "quic",
        "rcvr",
        "recv",
        "rtcp",
        "rustls",
        "sctp",
        "sndr",
        "tokio",
        "tungstenite",
        "uuid"
    ],
}